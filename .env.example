# PostgreSQL Database
DATABASE_URL=postgresql://user:password@localhost:5432/carematch

# NextAuth
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=  # openssl rand -base64 32 로 생성

# Kakao OAuth
# https://developers.kakao.com 에서 발급
KAKAO_CLIENT_ID=
KAKAO_CLIENT_SECRET=

# Naver OAuth
# https://developers.naver.com 에서 발급
NAVER_CLIENT_ID=
NAVER_CLIENT_SECRET=

# Kakao Alimtalk
KAKAO_ALIMTALK_KEY=
